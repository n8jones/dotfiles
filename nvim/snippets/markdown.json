{
  "journal": {
    "prefix": ["journal"],
    "body": [
      "---",
      "date: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE $CURRENT_HOUR:$CURRENT_MINUTE",
      "---",
      "# $CURRENT_DAY_NAME $CURRENT_MONTH_NAME $CURRENT_DATE, $CURRENT_YEAR",
      "",
      "## Metadata",
      "",
      "- tags:",
      "\t- #Journal",
      "\t- #${1:OnSite}",
      "",
      "## Time Log",
      "",
      "- $CURRENT_HOUR:$CURRENT_MINUTE Communications"
    ]
  },
  "quicknote": {
    "prefix": ["quicknote"],
    "body": [
      "---",
      "date: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE $CURRENT_HOUR:$CURRENT_MINUTE",
      "---",
      "# $1",
      "",
      "## Metadata",
      "",
      "- tags:",
      "\t- #$2",
      "",
      "$0"
    ]
  }
}
